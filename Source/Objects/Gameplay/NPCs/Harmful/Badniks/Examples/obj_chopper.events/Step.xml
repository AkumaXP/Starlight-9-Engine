<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<event category="STEP" id="0">
  <actions>
    <action id="603" library="1">
      <!--action name: Code-->
      <kind>CODE</kind>
      <allowRelative>false</allowRelative>
      <question>false</question>
      <canApplyTo>true</canApplyTo>
      <actionType>CODE</actionType>
      <functionName/>
      <relative>false</relative>
      <not>false</not>
      <appliesTo>.self</appliesTo>
      <arguments>
        <argument kind="STRING">/// Jump/Fall.

// Apply Gravity:
y_speed += y_accel;

// Check if we're at our starting position. If so, jump again.
if (y &gt;= ystart) y_speed = y_jump;

// Update Y position.
y += y_speed;
</argument>
      </arguments>
    </action>
  </actions>
</event>
